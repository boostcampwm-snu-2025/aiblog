{
	"name": "server",
	"version": "1.0.0",
	"description": "ai blog server",
	"keywords": [],
	"license": "ISC",
	"author": "Kreatinj <kjune1999@naver.com> (https://github.com/kreatinj)",
	"type": "module",
	"main": "dist/app.js",
	"scripts": {
		"build": "tsc",
		"check": "biome check --write",
		"clean": "rm -rf dist node_modules",
		"dev": "nodemon",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"reporter": "biome check --reporter=summary",
		"start": "node dist/app.js"
	},
	"dependencies": {
		"@google/genai": "^1.29.1",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"octokit": "^5.0.5",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@octokit/types": "^16.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/node": "^24.10.0",
		"jiti": "^2.6.1",
		"nodemon": "^3.1.10",
		"typescript": "^5.9.3"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"onFail": "error"
		},
		"packageManager": {
			"name": "pnpm",
			"onFail": "error",
			"version": "10.12.1"
		}
	}
}
